@{
    Layout = null;
}

<!DOCTYPE html>

<html>
    <head>
        <script type="text/javascript">
            function setupConsole() {
                $(".consoleContainer").retroConsole();
                $(".consoleContainer").retroConsole('mkdir', 'foo/blah/fish/qux');
                $(".consoleContainer").retroConsole('mkdir', 'zee');
                $(".consoleContainer").retroConsole('mkdir', 'blu');
                $(".consoleContainer").retroConsole('addExecutable', 'foo', 'dosomething.bat', '03/22/2011','01:24:06', function () { alert('didsomething!') });
                $(".consoleContainer").retroConsole('addExecutable', 'foo/blah/fish', 'doit.bat', '03/22/2011','01:24:06', function () { alert('didit!') });
            }
        </script>
        <script src="http://ajax.googleapis.com/ajax/libs/jquery/1.7.1/jquery.min.js" type="text/javascript"></script>
        <script src="/js/jquery.retroui.dos.js" type="text/javascript"></script>
        <link href="/css/jquery.retro.css" rel="stylesheet" type="text/css" />
        <script type="text/javascript">
            $(document).ready(function () {
                setupConsole();
            });
        </script>
        <title>title</title>
    </head>
    <body>
        <div style="padding:20px; width:100%; height:100%;">
            <div class="consoleContainer" id="foo" style="width:800px; height:800px;">
            </div>
        </div>
    </body>
</html>